<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from RequestHandlerSOAService WSDL service -->

  <service name="RequestHandlerSOA" provider="java:RPC" style="document" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://DefaultNamespace"/>
      <parameter name="wsdlServiceElement" value="RequestHandlerSOAService"/>
      <parameter name="schemaQualified" value="http://DefaultNamespace"/>
      <parameter name="wsdlServicePort" value="RequestHandlerSOA"/>
      <parameter name="className" value="DefaultNamespace.RequestHandlerSOASoapBindingImpl"/>
      <parameter name="wsdlPortType" value="RequestHandlerSOA"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getFactorial" qname="getFactorial" returnQName="retNS:getFactorialReturn" xmlns:retNS="http://DefaultNamespace" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:n" xmlns:pns="http://DefaultNamespace" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getFactorial"/>

  </service>
</deployment>
